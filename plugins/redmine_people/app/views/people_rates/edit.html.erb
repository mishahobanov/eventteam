<h2><%=l(:label_people_rate_edit)%></h2>

<%= labelled_form_for @people_rate, url: person_people_rate_path(@person, @people_rate), html: { id: 'people_rate', multipart: true } do |f| %>
  <%= error_messages_for 'people_rate' %>
  <div class="box tabular">
    <div id="all_attributes">
      <%= render partial: 'form', locals: { f: f } %>
    </div>
  </div>

  <%= submit_tag l(:button_update) %>
<% end %>
