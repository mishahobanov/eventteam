<h2><%=l(:label_people_rate_new)%></h2>

<%= labelled_form_for @people_rate, url: person_people_rates_path(@person), html: { id: 'people_rate', multipart: true } do |f| %>
  <%= error_messages_for 'people_rate' %>
  <div class="box tabular">
    <div id="all_attributes">
      <%= render partial: 'form', locals: { f: f } %>
    </div>
  </div>

  <%= submit_tag l(:button_create) %>
<% end %>
